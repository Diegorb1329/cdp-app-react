# CDP Configuration
VITE_CDP_PROJECT_ID=example-id

# Ethereum Account type: "eoa" for regular accounts, "smart" for Smart Accounts (gasless transactions and improved UX)
VITE_CDP_CREATE_ETHEREUM_ACCOUNT_TYPE=eoa
# Whether to create a Solana Account
VITE_CDP_CREATE_SOLANA_ACCOUNT=false

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Mapbox Configuration
# Note: Vite requires VITE_ prefix for client-side access
VITE_MAPBOX_API_KEY=your-mapbox-token
# Alternative (if using server-side only): MAPBOX_API_KEY=your-mapbox-token

# Pinata IPFS Configuration
# Note: Vite requires VITE_ prefix for client-side access
# The new Pinata SDK uses JWT tokens instead of API key/secret
VITE_PINATA_JWT=your-pinata-jwt-token
# Alternative (if using server-side only): PINATA_JWT=...
# Optional: Pinata Group ID for organizing uploads (leave empty to skip group assignment)
VITE_PINATA_GROUP_ID=your-pinata-group-id
